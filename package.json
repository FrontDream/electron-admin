{
  "name": "nanxu-admin-desktop",
  "version": "0.0.1",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "start": "umi dev",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp && electron-builder install-app-deps",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "svgo": "svgo -f src/assets/iconsvg --config=src/assets/iconsvg/svgo.yml",
    "postuninstall": "electron-builder install-app-deps",
    "electron:dev": "umi dev electron",
    "electron:build:win": "umi build electron --win",
    "electron:build:mac": "umi build electron --mac",
    "electron:build:linux": "umi build electron --linux",
    "rebuild-deps": "electron-builder install-app-deps",
    "electron:init": "umi electron init"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-descriptions": "^1.10.0",
    "@ant-design/pro-form": "^1.64.0",
    "@ant-design/pro-layout": "^6.35.0",
    "@ant-design/pro-table": "^2.71.0",
    "antd": "^4.24.8",
    "classnames": "^2.3.0",
    "electron-log": "^5.0.0-beta.16",
    "file-saver": "^2.0.5",
    "inversify": "^6.0.1",
    "jszip": "^3.10.1",
    "knex": "^2.3.0",
    "moment": "^2.29.0",
    "rc-menu": "^9.1.0",
    "rc-util": "^5.16.0",
    "react": "^17.0.0",
    "react-dev-inspector": "^1.7.0",
    "react-dom": "^17.0.0",
    "react-fittext": "^1.0.0",
    "react-helmet-async": "^1.2.0",
    "react-router": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.4",
    "umi": "^3.5.0",
    "umi-serve": "^1.9.10",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.0",
    "@playwright/test": "^1.17.0",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.0",
    "@types/jest": "^26.0.0",
    "@types/node": "^12.0.12",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@umijs/fabric": "^2.8.0",
    "@umijs/openapi": "^1.3.0",
    "@umijs/plugin-blocks": "^2.2.0",
    "@umijs/plugin-esbuild": "^1.4.0",
    "@umijs/plugin-openapi": "^1.3.0",
    "@umijs/preset-ant-design-pro": "^1.3.0",
    "@umijs/preset-dumi": "^1.1.0",
    "@umijs/preset-react": "^2.1.0",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.3.0",
    "detect-installer": "^1.0.0",
    "electron": "^11.0.1",
    "electron-builder": "23.0.3",
    "electron-webpack": "^2.8.2",
    "electron-webpack-ts": "^4.0.1",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "express": "^4.17.1",
    "gh-pages": "^3.2.0",
    "husky": "^7.0.4",
    "jsdom-global": "^3.0.0",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.1.0",
    "prettier": "^2.8.4",
    "stylelint": "^13.0.0",
    "svg-sprite-loader": "^5.2.1",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.2",
    "swagger-ui-react": "^3.52.0",
    "typescript": "^4.5.0",
    "umi-plugin-electron-builder": "2.1.14",
    "umi-serve": "^1.9.10"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "electronWebpack": {
    "renderer": null
  },
  "main": "main.js"
}
